{
	"name": "pierotto/symfony-middleware-bundle",
	"description": "This Symfony extension allows for the integration of middleware into your application, providing a streamlined approach to handling HTTP requests and responses.",
	"keywords": [
		"middleware",
		"symfony",
		"psr-15"
	],
	"type": "symfony-bundle",
	"license": "MIT",
	"authors": [
		{
			"name": "Petr Gala"
		}
	],
	"require": {
		"php": ">=8.1",
		"symfony/dependency-injection": "^6.2|^7.0",
		"symfony/event-dispatcher": "^6.2|^7.0",
		"symfony/http-kernel": "^6.2|^7.0",
		"symfony/config": "^6.2|^7.0",
		"symfony/http-foundation": "^6.2|^7.0",
		"psr/http-server-middleware": "^1.0",
		"psr/http-server-handler": "^1.0",
		"symfony/psr-http-message-bridge": "^2.1|^6.4|^7.0",
		"nyholm/psr7": "^1.5",
		"symfony/cache": "^6.2|^7.0"
	},
	"autoload": {
		"psr-4": {
			"Pierotto\\MiddlewareBundle\\": "src/"
		}
	}
}
